<?xml version='1.0' encoding='utf-8'?>
<widget id="com.konterbont.boxentaesch" version="0.7.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Lëtzebuergesch fir an d'Boxentäsch</name>
    <description>
       Lëtzebuergesch fir an d'Boxentäsch ass eng Interaktiv Applikatioun fir d'Reegelen vun der Lëtzebuergescher Sprooch besser ze verstoën.
    </description>
    <author email="infomedia@autisme.lu" href="http://www.autisme.lu">
        Info-Média - Autisme Luxembourg
    </author>
    <preference name="pgb-builder-version" value="1" />
    <preference name="phonegap-version" value="cli-7.0.1" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <platform name="android">
        <icon density="ldpi" src="ressourceimages/icons/android/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="ressourceimages/icons/android/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="ressourceimages/icons/android/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="ressourceimages/icons/android/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="ressourceimages/icons/android/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="ressourceimages/icons/android/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="ressourceimages/screens/android/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="ressourceimages/screens/android/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="ressourceimages/screens/android/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="ressourceimages/screens/android/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="ressourceimages/screens/android/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="ressourceimages/screens/android/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="ressourceimages/screens/android/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="ressourceimages/screens/android/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="ressourceimages/screens/android/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="ressourceimages/screens/android/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="ressourceimages/screens/android/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="ressourceimages/screens/android/drawable-port-xxxhdpi-screen.png" />
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <icon height="57" platform="ios" src="ressourceimages/icons/ios/icon.png" width="57" />
        <icon height="114" platform="ios" src="ressourceimages/icons/ios/icon@2x.png" width="114" />
        <icon height="40" platform="ios" src="ressourceimages/icons/ios/icon-40.png" width="40" />
        <icon height="80" platform="ios" src="ressourceimages/icons/ios/icon-40@2x.png" width="80" />
        <icon height="50" platform="ios" src="ressourceimages/icons/ios/icon-50.png" width="50" />
        <icon height="100" platform="ios" src="ressourceimages/icons/ios/icon-50@2x.png" width="100" />
        <icon height="60" platform="ios" src="ressourceimages/icons/ios/icon-60.png" width="60" />
        <icon height="120" platform="ios" src="ressourceimages/icons/ios/icon-60@2x.png" width="120" />
        <icon height="180" platform="ios" src="ressourceimages/icons/ios/icon-60@3x.png" width="180" />
        <icon height="72" platform="ios" src="ressourceimages/icons/ios/icon-72.png" width="72" />
        <icon height="144" platform="ios" src="ressourceimages/icons/ios/icon-72@2x.png" width="144" />
        <icon height="76" platform="ios" src="ressourceimages/icons/ios/icon-76.png" width="76" />
        <icon height="152" platform="ios" src="ressourceimages/icons/ios/icon-76@2x.png" width="152" />
        <icon height="29" platform="ios" src="ressourceimages/icons/ios/icon-small.png" width="29" />
        <icon height="58" platform="ios" src="ressourceimages/icons/ios/icon-small@2x.png" width="58" />
        <icon height="87" platform="ios" src="ressourceimages/icons/ios/icon-small@3x.png" width="87" />
        <splash height="1136" platform="ios" src="ressourceimages/screens/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" platform="ios" src="ressourceimages/screens/ios/Default-667h.png" width="750" />
        <splash height="2208" platform="ios" src="ressourceimages/screens/ios/Default-736h.png" width="1242" />
        <splash height="1242" platform="ios" src="ressourceimages/screens/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" platform="ios" src="ressourceimages/screens/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="768" platform="ios" src="ressourceimages/screens/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" platform="ios" src="ressourceimages/screens/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="1024" platform="ios" src="ressourceimages/screens/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="960" platform="ios" src="ressourceimages/screens/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="ressourceimages/screens/ios/Default~iphone.png" width="320" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="BackupWebStorage" value="none" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-console" spec="~1.0.7" />
    <plugin name="cordova-plugin-media" spec="~3.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.2" />
    <engine name="ios" spec="~4.4.0" />
    <engine name="android" spec="~6.2.3" />
    <engine name="browser" spec="~4.1.0" />
</widget>
